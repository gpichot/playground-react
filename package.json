{
  "name": "playground",
  "version": "0.1.0",
  "author": {
    "name": "Gabriel Pichot"
  },
  "scripts": {
    "build": "npm run typecheck && vite build",
    "commit": "git-cz",
    "dev": "vite",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}' && git update-index --again",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "serve": "npm run build && vite preview --port 3000",
    "test:e2e": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress run'",
    "test:unit": "jest --watch",
    "test:unit:ci": "jest",
    "test:unit:coverage": "jest --silent --watchAll=false --coverage",
    "typecheck": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{css,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@artsy/fresnel": "^3.5.0",
    "@hookform/resolvers": "^2.8.8",
    "classnames": "^2.3.1",
    "pokedex-promise-v2": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.30.0",
    "react-query": "^3.34.19",
    "react-router-dom": "6.3.0",
    "react-use": "^17.3.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.2",
    "styled-components": "^5.3.5",
    "use-deep-compare-effect": "^1.8.1",
    "uuidv4": "^6.2.13",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/cz-commitlint": "^16.2.3",
    "@cypress/vite-dev-server": "^2.2.2",
    "@hookform/devtools": "^4.1.0",
    "@storybook/addon-a11y": "^6.4.21",
    "@storybook/addon-actions": "^6.5.0-alpha.60",
    "@storybook/addon-essentials": "^6.5.0-alpha.60",
    "@storybook/addon-interactions": "^6.5.0-alpha.60",
    "@storybook/addon-links": "^6.5.0-alpha.60",
    "@storybook/builder-webpack5": "^6.5.0-alpha.60",
    "@storybook/manager-webpack5": "^6.5.0-alpha.60",
    "@storybook/react": "^6.5.0-alpha.60",
    "@storybook/testing-library": "^0.0.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "18",
    "@types/react-dom": "18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "commitizen": "^4.2.4",
    "cypress": "^9.5.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.10",
    "eslint-plugin-testing-library": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.8",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-nesting": "^10.1.4",
    "prettier": "^2.6.2",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "sort-package-json": "^1.55.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "webpack": "5"
  },
  "resolutions": {
    "postcss": "8"
  }
}
